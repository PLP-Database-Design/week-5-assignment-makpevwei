<!DOCTYPE html>
<html>
<head>
    <title>Providers Data</title>
</head>
<body>
    <h1>Provider List</h1>
    <table>
        <thead>
            <tr>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Provider Specialty</th>
            </tr>
        </thead>
        <tbody>
            <% results.forEach((provider) => { %>
                <tr>
                    <td><%= provider.first_name %></td>
                    <td><%= provider.last_name %></td>
                    <td><%= provider.provider_specialty %></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
</body>
</html>
